{
    "name": "iazaran/php-mvc",
    "description": "Pure PHP Composer based MVC Framework",
    "type": "project",
    "authors": [
        {
            "name": "Ismael Azaran",
            "email": "eazaran@gmail.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "files": [
            "env.php",
            "src/migrations.php",
            "src/routes.php"
        ],
        "classmap": [
            "src/"
        ]
    },
    "config": {
        "platform": {
            "php": "8.0"
        }
    },
    "require": {
        "php": ">=8.0",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-gd": "*",
        "ext-iconv": "*"
    }
}
